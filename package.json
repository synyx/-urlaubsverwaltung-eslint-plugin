{
	"name": "@urlaubsverwaltung/eslint-plugin",
	"version": "0.1.0",
	"description": "custom eslint rules for the open source urlaubsverwaltung application",
	"main": "index.js",
	"scripts": {
		"test": "ava"
	},
	"keywords": [
		"eslint",
		"eslintplugin",
		"eslint-plugin",
		"urlaubsverwaltung"
	],
	"publishConfig": {
		"access": "public"
	},
	"author": "Benjamin Seber <seber@synyx.de> (https://www.synyx.de)",
	"license": "Apache-2.0",
	"peerDependencies": {
		"eslint": ">= 5"
	},
	"devDependencies": {
		"ava": "1.4.1",
		"eslint": "5.16.0",
		"eslint-ava-rule-tester": "3.0.0",
		"husky": "1.3.1",
		"lint-staged": "8.1.5",
		"prettier": "1.17.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "npm test"
		}
	},
	"lint-staged": {
		"*.js": [
			"prettier --write '**/*.js'",
			"git add"
		]
	},
	"ava": {
		"files": [
			"test/*.js"
		]
	}
}
