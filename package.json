{
  "name": "@urlaubsverwaltung/eslint-plugin",
  "version": "1.1.0",
  "description": "custom eslint rules for the open source urlaubsverwaltung application",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "urlaubsverwaltung"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Benjamin Seber <seber@synyx.de> (https://www.synyx.de)",
  "license": "Apache-2.0",
  "peerDependencies": {
    "eslint": ">= 6 < 7"
  },
  "devDependencies": {
    "ava": "3.14.0",
    "eslint": "6.2.2",
    "eslint-ava-rule-tester": "3.0.0",
    "husky": "2.7.0",
    "lint-staged": "8.2.1",
    "prettier": "1.18.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write '**/*.js'",
      "git add"
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  }
}
