{
  "name": "@urlaubsverwaltung/eslint-plugin",
  "version": "1.2.0",
  "description": "custom eslint rules for the open source urlaubsverwaltung application",
  "main": "index.js",
  "scripts": {
    "test": "ava"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "eslint-plugin",
    "urlaubsverwaltung"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Benjamin Seber <seber@synyx.de> (https://www.synyx.de)",
  "license": "Apache-2.0",
  "peerDependencies": {
    "eslint": "*"
  },
  "devDependencies": {
    "ava": "3.15.0",
    "eslint": "8.12.0",
    "eslint-ava-rule-tester": "4.0.0",
    "husky": "5.1.3",
    "lint-staged": "10.5.4",
    "prettier": "2.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm test"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier --write '**/*.js'"
    ]
  },
  "ava": {
    "files": [
      "test/*.js"
    ]
  }
}
